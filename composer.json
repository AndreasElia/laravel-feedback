{
    "name": "andreaselia/feedback",
    "description": "Feedback widget for the Laravel framework.",
    "keywords": ["laravel", "feedback", "package", "widget"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Elia",
            "email": "contact@andreaselia.co.uk"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-intl": "*",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/collections": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/view": "^8.0",
        "jenssegers/agent": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "AndreasElia\\Feedback\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndreasElia\\Feedback\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AndreasElia\\Feedback\\FeedbackServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "6.0",
        "phpunit/phpunit": "^9.5"
    }
}
